{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ingeal-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/ingeal-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/ingeal-api",
  "projects": {
    "ingeal-api": {
      "type": "application",
      "root": "apps/ingeal-api",
      "entryFile": "main",
      "sourceRoot": "apps/ingeal-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ingeal-api/tsconfig.app.json"
      }
    },
    "microservice": {
      "type": "application",
      "root": "apps/microservice",
      "entryFile": "main",
      "sourceRoot": "apps/microservice/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservice/tsconfig.app.json"
      }
    }
  }
}